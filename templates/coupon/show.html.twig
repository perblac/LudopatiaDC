{% extends 'base.html.twig' %}

{% block title %}Coupon{% endblock %}

{% block body %}
    <h1>Coupon</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ coupon.id }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ coupon.number }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ coupon.state }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_coupon_index') }}">back to list</a>

    <a href="{{ path('app_coupon_edit', {'id': coupon.id}) }}">edit</a>

    {{ include('coupon/_delete_form.html.twig') }}
